  "description": "node with type script demo",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "env-cmd -f .env.development nodemon src/index.ts",
    "local": "env-cmd -f .env nodemon src/index.ts",
    "prod": "nodemon src/index.ts",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "seed": "npx sequelize-cli db:seed:all",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo ",
    "seed:superadmin": "ts-node src/seeds/create-super-user.ts",
    "seed:admin": "ts-node src/seeds/create-admin-user.ts",
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "clean:js": "find ./src -name \"*.js\" -type f -delete"
  },
